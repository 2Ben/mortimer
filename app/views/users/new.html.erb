<h2>Create a new user</h2>
<% @user.password = @user.password_confirmation = nil %>

<%= error_messages_for :user %>

<% form_for :user, :url => users_path do |f| -%>
  <%= render :partial => "form" %>
  
  <p><%= submit_tag "Create" %> | 
  <a href="<%= users_path %>">Cancel</a></p>
<% end -%>
