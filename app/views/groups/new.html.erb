<h1>New Group</h1>

<% form_for :group, :url => groups_url do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form" %>
  
	<div class="buttons clear">
		<%#=f.submit "Create" %>
		<button class="btn_create">Create</button>
		<a href="<%= home_path %>" class="cancel btn_cancel">Cancel</a>
	</div>
<% end %>

